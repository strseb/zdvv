{
  "Kernel": "kernel",
  "Boot": "nocloud",
  "RunConfig": {
    "Ports": [
      "8443:8443/tcp"
    ],
    "Env": [
      "JWT_SECRET=${JWT_SECRET}",
      "ADMIN_TOKEN=${ADMIN_TOKEN}"
    ]
  },  "Args": {
    "addr": ":8443",
    "cert": "server.crt",
    "key": "server.key",
    "insecure": "${INSECURE:-false}",
    "no-http2": "${NO_HTTP2:-false}",
    "no-http3": "${NO_HTTP3:-false}"
  },
  "ManifestPassthrough": {
    "name": "zdvv",
    "description": "Zentrale Datenverkehrsvermittlung - HTTP/2 and HTTP/3 Proxy Service"
  }
}
