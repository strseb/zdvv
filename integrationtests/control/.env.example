# ZDVV Control Server Integration Tests - Environment Variables
# Copy this file to .env in the root directory and adjust values as needed

# Control server URL for integration tests
ZDVV_CONTROL_URL=http://localhost:8080

# API key for authentication
ZDVV_API_KEY=my-integration-test-api-key

# Set to true to enable detailed test logging
ZDVV_TEST_DEBUG=false

# Optional: Timeout configuration for HTTP requests in seconds
ZDVV_HTTP_TIMEOUT=10
